<?xml version="1.0" encoding="utf-8"?>
<OJP xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns="http://www.siri.org.uk/siri" version="1.0" xmlns:ojp="http://www.vdv.de/ojp" xsi:schemaLocation="http://www.siri.org.uk/siri ../ojp-xsd-v1.0/OJP.xsd">
    <OJPRequest>
        <ServiceRequest>
            <RequestTimestamp>${timestamp}</RequestTimestamp>
            <RequestorRef>ojpch-performance-test</RequestorRef>
            <ojp:OJPTripRequest>
                <RequestTimestamp>${timestamp}</RequestTimestamp>
                <ojp:Origin>
                   <ojp:PlaceRef>
                        <ojp:StopPlaceRef>${o_didok}</ojp:StopPlaceRef>
                        <ojp:LocationName>
                            <ojp:Text>${o_name}</ojp:Text>
                        </ojp:LocationName>
                    </ojp:PlaceRef>                
                    <ojp:DepArrTime>${arrdep}</ojp:DepArrTime>
                </ojp:Origin>
                <ojp:Destination>
                   <ojp:PlaceRef>
                        <ojp:StopPlaceRef>${d_didok}</ojp:StopPlaceRef>
                        <ojp:LocationName>
                            <ojp:Text>${d_name}</ojp:Text>
                        </ojp:LocationName>
                    </ojp:PlaceRef>
                </ojp:Destination>${via}
                <ojp:Params>
                    <ojp:NumberOfResults>${tr_number_of_results}</ojp:NumberOfResults>
                    <ojp:IncludeTrackSections>${tr_include_track_sections}</ojp:IncludeTrackSections>
                    <ojp:IncludeLegProjection>${tr_include_leg_projection}</ojp:IncludeLegProjection>
                    <ojp:IncludeTurnDescription>${tr_include_turn_description}</ojp:IncludeTurnDescription>
                    <ojp:IncludeIntermediateStops>${tr_include_intermediate_stops}</ojp:IncludeIntermediateStops>
                </ojp:Params>
            </ojp:OJPTripRequest>
        </ServiceRequest>
    </OJPRequest>
</OJP>
