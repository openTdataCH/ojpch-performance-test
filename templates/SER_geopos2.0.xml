<?xml version="1.0" encoding="UTF-8"?>
<OJP xmlns="http://www.vdv.de/ojp" xmlns:siri="http://www.siri.org.uk/siri" version="2.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.vdv.de/ojp OJP_changes_for_v1.1/OJP.xsd">
    <OJPRequest>
        <siri:ServiceRequest>
            <siri:RequestTimestamp>${timestamp}</siri:RequestTimestamp>
            <siri:RequestorRef>ojpch-performance-test</siri:RequestorRef>
            <OJPStopEventRequest>
                <siri:RequestTimestamp>${timestamp}</siri:RequestTimestamp>
                <siri:MessageIdentifier>SER-3a</siri:MessageIdentifier>
                <Location>
                    <PlaceRef>
                        <GeoPosition>
                            <siri:Longitude>${o_x}</siri:Longitude>
                            <siri:Latitude>${o_y}</siri:Latitude>
                        </GeoPosition>
                        <Name>
                            <Text>Name</Text>
                        </Name>
                    </PlaceRef>
                </Location>
                <Params>
                    <OperatorFilter>
                        <Exclude>${ser_operator_exclude}</Exclude>
                        <OperatorRef>${ser_operator_ref}</OperatorRef>
                    </OperatorFilter>
                    <NumberOfResults>${ser_number_of_results}</NumberOfResults>
					<StopEventType>${ser_stop_event_type_reference}</StopEventType>
					<IncludePreviousCalls>${ser_include_previous_calls}</IncludePreviousCalls>
					<IncludeOnwardCalls>${ser_include_onward_calls}</IncludeOnwardCalls>
					<UseRealtimeData>${ser_include_realtime_data2.0}</UseRealtimeData>
				</Params>
            </OJPStopEventRequest>
        </siri:ServiceRequest>
    </OJPRequest>
</OJP>
